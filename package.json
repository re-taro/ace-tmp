{
  "name": "ace-tmp",
  "version": "0.0.0",
  "description": "this repository is a temporary repository for CyberAgent ACE",
  "private": true,
  "packageManager": "pnpm@8.7.3",
  "type": "module",
  "scripts": {
    "gen": "scaffdog generate",
    "start": "next start",
    "build": "run-s build:*",
    "build:next": "next build",
    "dev": "next dev",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "run-s -c lint:*",
    "lint:eslint": "next lint",
    "lint:stylelint": "stylelint './src/**/*.css'",
    "lint:tsc": "tsc --pretty --noEmit",
    "lint:prettier": "prettier --check './src/**/*.{ts,tsx,css}'",
    "lint:markuplint": "markuplint -p './src/**/*.tsx'",
    "lint-fix": "run-s -c lint-fix:*",
    "lint-fix:eslint": "pnpm run lint:eslint --fix",
    "lint-fix:stylelint": "pnpm run lint:stylelint --fix",
    "lint-fix:prettier": "prettier --write './src/**/*.{ts,tsx,css}'",
    "test": "run-s -c test:*",
    "test:unit": "vitest run",
    "test:interaction": "pnpm run storybook:build && run-p -r storybook:start storybook:test",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:start": "http-server -s -p 9001 storybook-static",
    "storybook:test": "test-storybook --url http://localhost:9001 --coverage"
  },
  "dependencies": {
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@markuplint/jsx-parser": "3.9.0",
    "@markuplint/react-spec": "3.10.0",
    "@next/bundle-analyzer": "13.4.19",
    "@storybook/addon-a11y": "7.4.0",
    "@storybook/addon-coverage": "0.0.9",
    "@storybook/addon-essentials": "7.4.0",
    "@storybook/addon-interactions": "7.4.0",
    "@storybook/addon-links": "7.4.0",
    "@storybook/blocks": "7.4.0",
    "@storybook/jest": "0.2.2",
    "@storybook/nextjs": "7.4.0",
    "@storybook/react": "7.4.0",
    "@storybook/test-runner": "0.13.0",
    "@storybook/testing-library": "0.2.0",
    "@tsconfig/strictest": "2.0.2",
    "@types/node": "20.5.9",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@vitejs/plugin-react": "4.0.4",
    "autoprefixer": "10.4.15",
    "axe-playwright": "1.2.3",
    "cross-env": "7.0.3",
    "cssnano": "6.0.1",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-storybook": "0.6.13",
    "eslint-plugin-strict-dependencies": "1.2.1",
    "eslint-plugin-unused-imports": "3.0.0",
    "http-server": "14.1.1",
    "markuplint": "3.12.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "postcss": "8.4.29",
    "prettier": "3.0.3",
    "scaffdog": "3.0.0",
    "storybook": "7.4.0",
    "stylelint": "15.10.3",
    "stylelint-config-recess-order": "4.3.0",
    "stylelint-config-recommended": "13.0.0",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.34.3"
  }
}
